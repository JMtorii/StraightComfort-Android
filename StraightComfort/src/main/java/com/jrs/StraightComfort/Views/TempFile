	ArrayList<Contents> filter = new ArrayList<Contents>();
	ArrayList<Contents> contents = new ArrayList<Contents>();
	ArrayList<ArrayList<String>> finalString = new ArrayList<ArrayList<String>>();
	int counter1 = 0;
	int counter2 = 0;
	for (Contents f :filter)
	{
		

		for (Contents t: contents)
		{
			ArrayList<String> eachOne = new ArrayList<String>();
			
			if (f.getTitle()==t.getTitle()||f.getTitle()=="all")
			{
				if (counter1 == 0 && counter2 == 0)
				{
					eachOne.add("INTRO");
				}
				
				for (String contentPages: t.getPages())
				{
					for (String filterPages: f.getPages())
					{
						if (filterPages == contentPages || filterPages == "-1")
						{
							eachOne.add(contentPages);
						}
					}
				}			
				eachOne.add("In Between");	
				finalString.add(eachOne);
				counter2++;
			}
			
		}
		counter1++;
		counter2=0;
	}
		finalString.get(finalString.size()-1).set(finalString.get(finalString.size()-1).size()-1,"END");
